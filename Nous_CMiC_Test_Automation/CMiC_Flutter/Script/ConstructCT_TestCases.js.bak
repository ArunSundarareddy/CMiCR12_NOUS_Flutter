/*###############################################################################################################################################################
  C118763 CT:Constrcut Crew Time - Open Navigation Menu
###############################################################################################################################################################*/
function C118763()
{
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
  let dictTestData = ReadXlInputDataValues("ConstructCT", "C118763")
//
  ProjectSuite.Variables.TestCaseID = dictTestData.Item("TestRailTCID")
  Log.Message("TestCaseID: "+ProjectSuite.Variables.TestCaseID)
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("CT:Constrcut Crew Time - Open Navigation Menu");
//----Pre-Requisite: Handling via direct URL---------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("Pre-requisite: Login to Construct CT") 
  CMiC_LoginDirectURL()
  CloseFolder();
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
  EnableFlutterAccessibility_CT()
//-----Step - 1--------------------------------------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("1: Click on Navigation Menu Bar") 
  SetBrowserZoom100()
  WaitObjLoad(CCT_btnNavigationMenuBar,CCT_ShadowRoot)
  Click(CCT_btnNavigationMenuBar,"Click on Navigation Menu Bar",CCT_ShadowRoot);
  WaitObjLoad(CCT_btnLogout,CCT_ShadowRoot)
  CloseFolder();
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("Verify if User can open navigation menu") 
  Exists(CCT_btnLogout,"User can open navigation menu",CCT_ShadowRoot)
  CloseFolder();
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CMiC_ConstructCTLogout()
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CloseFolder();
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
}
/*#############################################################################################################################################################*/


