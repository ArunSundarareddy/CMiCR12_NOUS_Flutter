/*###############################################################################################################################################################
  C118765 CT: Crews - Create Crew
###############################################################################################################################################################*/
function C118765()
{
//--------- -----------------------------------------------------------------------------------------------------------------------------------------------------
  let dictTestData = ReadXlInputDataValues("ConstructCT", "C118765")

  ProjectSuite.Variables.TestCaseID = dictTestData.Item("TestRailTCID")
  Log.Message("TestCaseID: "+ProjectSuite.Variables.TestCaseID)
  //---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("CT: Crews - Create Crew");

  CreateStepFolder("Pre-requisite: Login to Construct CT") 
  CMiC_LoginDirectURL()
  EnableFlutterAccessibility_CT()
  SetBrowserZoom100()
  CloseFolder();
  //---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("1: Click on Navigation Menu Bar") 
  WaitObjLoad(CCT_btnNavigationMenuBar,CCT_ShadowRoot)
  Click(CCT_btnNavigationMenuBar,"Click on Navigation Menu Bar",CCT_ShadowRoot);
  WaitObjLoad(CCT_btnLogout,CCT_ShadowRoot)
  CloseFolder();
  //---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("2: Navigate to Create CREW Screen ") 
  Exists(CCT_btnLogout,"User can open navigation menu",CCT_ShadowRoot)
  Click("[aria-label='Crews']","Click on CREW Tab Menu",CCT_ShadowRoot);
  WaitUntilToLoad()
  //---------------------------------------------------------------------------------------------------------------------
  objButtonList = GetUIObjects("[role='button']",CCT_ShadowRoot)
  objButtonList[objButtonList.length-1].Click()
  WaitUntilToLoad()
  CloseFolder();
  //---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CreateStepFolder("3: Enter CREW details") 
  EnterCREWDetail()
  CloseFolder();
  //---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CMiC_ConstructCTLogout()
  //---------------------------------------------------------------------------------------------------------------------------------------------------------------
  CloseFolder();
//---------------------------------------------------------------------------------------------------------------------------------------------------------------
}
